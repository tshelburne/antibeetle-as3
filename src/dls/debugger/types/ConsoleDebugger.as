package dls.debugger.types {		import dls.debugger.types.IDebugger;	import dls.debugger.utils.DebugUtil;		import flash.external.ExternalInterface;		public class ConsoleDebugger implements IDebugger {				private var active:Boolean;		public function ConsoleDebugger(active:Boolean=false) {			this.active = active;		}				public function out(message:*):void {			if (this.active) {				ExternalInterface.call("console.log", DebugUtil.print(message));			}		}	}	}